public class Sample
{
    public static void main(String args[])
    {
        int a[]={1,3,4,5,6,2,3,4,7,8,5,6},n=10,i,count=0,flag=0;
        for(i=0;i<a.length;i++)
        {
            if(a[i]==n)
            {
                count++;
                flag=1;
            }
        }
    if(flag==1)
        System.out.println("The frequency of the given number-"+n+" is "+count);
    else
        System.out.println("The given number "+n+ " is not found in the array");
    }
}
