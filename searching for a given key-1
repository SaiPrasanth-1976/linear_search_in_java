public class Search
{
    public static void main(String args[])
    {
        int i,a[]={2,7,6,3,4,5,1};
        int n=7,flag=0;
        for(i=0;i<7;i++)
        {
            if(a[i]==n)
            {
                flag=1;
                break;
                
            }
        }
        if(flag==1)
        System.out.println("Index of the given key is "+i);
        else
                System.out.println("Given key is not in the array");
    }
}  
